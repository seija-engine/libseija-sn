<Panel isCanvas="true">
    <Image sprite="default.white" color="#e8e8e7" />
    <Track width="26" hor="Center" orientation="Ver" value="0.6">
        <Track.template>
            <ControlTemplate>
                <Image imageType="Slice" sprite="default.scale-vert-trough" color="#ffffff" />
            </ControlTemplate>
        </Track.template>
        <Track.thumb>
          <Thumb style="{Res SliderThumb}"/>
        </Track.thumb>
    </Track>
    
    <Text ver="End" height="50" margin="0,0,0,200" text="{Binding Data count}"/>
</Panel>